.register_button .login_button{
	border: 0;
	border-radius: 0;
	width: calc(80%);
	height: 45px;
	FILTER: progid:DXImageTransform.Microsoft.Gradient(gradientType=0,startColorStr=#86CF65,endColorStr=#63B53F); /*IE*/
	background: -moz-linear-gradient(top,#86CF65,#63B53F);/*火狐*/
	background: -webkit-gradient(linear,0% 0%,0% 100%,from(#86CF65),to(#63B53F));/*谷歌*/
	font-size: 20px;
	color: white;
}
.register_button_mousedown .login_button_mousedown{
	/*filter:alpha(opacity="90");
    Opacity:0.9;*/
   FILTER: progid:DXImageTransform.Microsoft.Gradient(gradientType=0,startColorStr=#8DDF65,endColorStr=#6DD53F);/*IE*/
   background: -moz-linear-gradient(top,#8DDF65,#6DD53F);/*火狐*/
   background: -webkit-gradient(linear,0% 0%,0% 100%,from(#8DDF65),to(#6DD53F));/*谷歌*/
}
